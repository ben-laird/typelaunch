{
  "name": "typelaunch-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/typelaunch-starter.umd.cjs",
  "module": "./dist/typelaunch-starter.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/typelaunch-starter.js",
      "require": "./dist/typelaunch-starter.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "vitest",
    "test-once": "vitest --run",
    "coverage": "vitest run --coverage && open-cli coverage/index.html",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@types/node": "^18.14.0",
    "@vitest/coverage-c8": "^0.28.5",
    "open-cli": "^7.1.0",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-plugin-dts": "2.0.0-beta.1",
    "vitest": "^0.28.5"
  },
  "files": [
    "dist"
  ]
}
